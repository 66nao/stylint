{
    "borderNone": true,
    "brackets": true,
    "colons": true,
    "commaSpace": true,
    "commentSpace": true,
    "cssLiteral": true,
    "depthLimit": 5,
    "efficient": true,
    "enforceVarStyle": true,
    "enforceBlockStyle": true,
    "extendPref": "extends",
    "indentSpaces": 4,
    "leadingZero": true,
    "maxWarnings": 10,
    "mixed": true,
    "namingConvention": "lowercase-dash",
    "parenSpace": true,
    "placeholders": true,
    "semicolons": true,
    "trailingWhitespace": true,
    "universal": true,
    "valid": true,
    "zeroUnits": true
}