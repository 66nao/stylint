{
    "brackets": true,
    "colons": true,
    "colors": true,
    "commaSpace": true,
    "commentSpace": true,
    "cssLiteral": true,
    "depthLimit": 5,
    "duplicates": true,
    "efficient": true,
    "emoji": true,
    "enforceBlockStyle": true,
    "enforceVarStyle": true,
    "extendPref": "@extends",
    "globalDupe": false,
    "indentPref": "tabs",
    "leadingZero": true,
    "maxWarnings": 10,
    "maxWarningsKill": false,
    "mixed": true,
    "namingConvention": "lowercase-dash",
    "namingConventionStrict": true,
    "noNone": true,
    "parenSpace": true,
    "placeholders": true,
    "quotePref": "single",
    "semicolons": true,
    "sortOrder": false,
    "stackedProperties": true,
    "universal": true,
    "valid": true,
    "whitespace": true,
    "zeroUnits": true,
    "zIndexNormalize": 5
}
