{
    // Comments - woooo
    "blocks": "always",
    "brackets": "always",
    "colons": "always",
    "colors": false,
    "commaSpace": "always",
    "commentSpace": "always",
    "cssLiteral": "never",
    "depthLimit": false,
    "duplicates": true,
    "efficient": "always",
    "extendPref": false,
    "globalDupe": true,
    // "indentPref": 2,
    "indentPref": {
        "expect": false
    },
    "leadingZero": "always",
    "maxErrors": false,
    "maxWarnings": false,
    "mixed": true,
    "mixins": ["test-mixin", "mixin"],
    "namingConvention": false,
    "namingConventionStrict": false,
    "none": "never",
    "noImportant": true,
    "parenSpace": false,
    "placeholders": "always",
    "prefixVarsWithDollar": "always",
    "quotePref": false,
    "reporter": "../core/reporter",
    "semicolons": "always",
    "sortOrder": "alphabetical",
    "stackedProperties": "never",
    "trailingWhitespace": "never",
    "universal": false,
    "valid": true,
    "zeroUnits": "never",
    "zIndexNormalize": false
}
